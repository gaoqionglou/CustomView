<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="CustomTitleView">
        <attr name="titleText" format="string" />
        <attr name="titleTextColor" format="color" />
        <attr name="titleTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleButton">
        <attr name="radius" format="dimension" />
        <attr name="backgroundColor" format="color" />
        <attr name="backgroundDrawable" format="reference" />
    </declare-styleable>
    <declare-styleable name="TestView">
        <attr name="testradius" format="dimension" />
        <attr name="testbackgroundd" format="reference|color" />
    </declare-styleable>
    <attr name="testViewStyle" format="reference" />
    <attr name="DefualtCircleButton" format="reference" />
    <declare-styleable name="Card">
        <attr name="size" format="dimension|reference" />
        <attr name="bgColor" format="color|reference" />
    </declare-styleable>
    <declare-styleable name="TimeScheduleView">
        <attr name="innerCirlceRadius" format="dimension" />
        <attr name="innerCircleColor" format="color" />
        <attr name="outterCirlceRadius" format="dimension" />
        <attr name="outterCirlceWidth" format="dimension" />
        <attr name="outterCircleColor" format="color" />
        <attr name="mbackgroundColor" format="color" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="progressCircleColor" format="color" />
        <attr name="progressCircleWidth" format="dimension" />
        <attr name="progressDrawable" format="reference" />
        <attr name="rippleWidth" format="dimension" />
        <attr name="rippleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RadianViewPager">
        <attr name="android:layout_margin"/>
        <attr name="android:padding"/>
        <attr name="android:background"/>
        <attr name="radianTop" format="dimension"/>
        <attr name="itemRadianTop" format="dimension"/>
        <attr name="itemRadianWidth" format="dimension"/>
        <attr name="itemRadianColor" format="color"/>
        <attr name="itemCircleRadius" format="color"/>
        <attr name="settledCircleCount" format="integer"/>
        <attr name="settledCircleText" format="string"/>
        <attr name="settledCircleRadius" format="dimension"/>
        <attr name="settledCircleBackgroud" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="CircleRadianButton">
        <attr name="android:layout_margin"/>
        <attr name="android:padding"/>
        <attr name="crbackground" format="color|reference"/>
        <attr name="radius"/>
    </declare-styleable>
    <declare-styleable name="CustomRelativeLauout">
        <attr name="circleMargin" format="dimension"/>
        <attr name="circleWidth" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="circleDrawable" format="reference"/>
    </declare-styleable>
</resources>